					"mode": "raw",
					"raw": "{{TokenRequest}}",
					"options": {
						"raw": {
							"language": "json"
						}
					}
				},
				"url": {
					"raw": "https://auth.linkhub.co.kr/POPBILL_TEST/Token",
					"protocol": "https",
					"host": [
						"auth",
						"linkhub",
						"co",
						"kr"
					],
					"path": [
						"POPBILL_TEST",
						"Token"
					]
				},
				"description": "ㆍPOPBill API 사용에 필요한 Token Request."
			},
			"response": []

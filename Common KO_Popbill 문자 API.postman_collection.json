							"var EDate = '20211210'",
							"pm.collectionVariables.set(\"Search_EDate\", EDate);",
							"",
							"// [배열] 전송상태 : \"1\" / \"2\" / \"3\" / \"4\" 중 선택 (다중 선택 가능)",
							"// └ 1 = 대기, 2 = 성공, 3 = 실패, 4 = 취소",
							"var State = ['1,2,3,4'];",
							"pm.collectionVariables.set(\"Search_State\", State.join(','));",
							"",
							"// [배열] 검색대상 : \"SMS\" / \"LMS\" / \"MMS\" 중 선택 (다중 선택 가능)",
							"// └ SMS = 단문, LMS = 장문, MMS = 포토문자",
							"var Item = ['SMS', 'LMS', 'MMS'];",
							"pm.collectionVariables.set(\"Search_Item\", Item.join(','));",
							"",
							"// 전송유형별 조회 : null / \"0\" / \"1\" 중 택 1",
							"// └ null = 전체, 0 = 즉시전송건, 1 = 예약전송건",
							"var ReserveYN = [];",
							"pm.collectionVariables.set(\"Search_ReserveYN\", ReserveYN);",
							"",
							"// 사용자권한별 조회 : true / false 중 택 1",
							"// └ true = 해당 담당자 계정으로 접수한 카카오톡만 조회 (개인권한), false = 접수한 카카오톡 전체 조회 (관리자권한)",
							"var SenderYN = true;",
							"pm.collectionVariables.set(\"Search_SenderYN\", SenderYN);",
							"",
							"// 목록 페이지번호",
							"var Page = 1;",
							"pm.collectionVariables.set(\"Search_Page\", Page);",
							"",
							"// 페이지당 표시할 목록 개수 (최대 1,000)",
							"var PerPage = 500;",
							"pm.collectionVariables.set(\"Search_PerPage\", PerPage);",
							"",
							"// 알림톡/친구톡 접수일시를 기준으로 하는 목록 정렬 방향 : \"D\" / \"A\" 중 택 1",
							"// └ D = 내림차순(기본값), A = 오름차순",
							"var Order = '';",
							"pm.collectionVariables.set(\"Search_Order\", Order);",
							"",
							"// \t조회하고자 하는 수신자명",
							"var QString = '';",
							"pm.collectionVariables.set(\"Search_QString\", encodeURIComponent(QString));"
						],
						"type": "text/javascript"
					}
				}
			],
